module Main exposing (main)

import Browser
import Html exposing (Html, div, h2, h3, text)
import Html.Attributes exposing (style)
import Octicons.Large as Large
import Octicons.Medium as Medium
import Octicons.Small as Small


type alias Model =
    {}


type Msg
    = NoMsg


main : Program () Model Msg
main =
    Browser.document
        { init = \_ -> ( {}, Cmd.none )
        , view = view
        , update = \_ model -> ( model, Cmd.none )
        , subscriptions = \_ -> Sub.none
        }


view : Model -> Browser.Document Msg
view model =
    { title = "Octicons.elm Sample"
    , body = [ viewBody model ]
    }


viewBody : Model -> Html Msg
viewBody model =
    div [ style "padding" "10px 300px 30px 300px" ]
        [ h2 [] [ text "Octicons.elm Sample" ]
        , viewSmallIcons model
        , viewMediumIcons model
        , viewLargeIcons model
        ]


viewSmallIcons : Model -> Html Msg
viewSmallIcons model =
    div []
        [ h3 []
            [ text "Octicons.Small" ]
        , div [ style "display" "flex", style "flex-wrap" "wrap", style "gap" "20px" ]
            [ Small.alertFill []
            , Small.checkCircleFill []
            , Small.chevronDown []
            , Small.chevronRight []
            , Small.noEntryFill []
            , Small.xCircleFill []
            ]
        ]


